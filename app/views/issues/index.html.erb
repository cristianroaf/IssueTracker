<p id="notice"><%= notice %></p>

<h1>Issues</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Type</th>
      <th>Priority</th>
      <th>Status</th>
      <th>User</th>
      <th>Asignee</th>
      <th>Votes</th>
      <th>Watchers</th>
      <th>At name</th>
      <th>At format</th>
      <th>At size</th>
      <th>At updated at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @issues.each do |issue| %>
      <tr>
        <td><%= issue.Title %></td>
        <td><%= issue.Description %></td>
        <td><%= issue.Type %></td>
        <td><%= issue.Priority %></td>
        <td><%= issue.Status %></td>
        <td><%= issue.user_id %></td>
        <td><%= issue.asignee_id %></td>
        <td><%= issue.Votes %></td>
        <td><%= issue.Watchers %></td>
        <td><%= issue.at_name %></td>
        <td><%= issue.at_format %></td>
        <td><%= issue.at_size %></td>
        <td><%= issue.at_updated_at %></td>
        <td><%= link_to 'Show', issue %></td>
        <td><%= link_to 'Edit', edit_issue_path(issue) %></td>
        <td><%= link_to 'Destroy', issue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Issue', new_issue_path %>
